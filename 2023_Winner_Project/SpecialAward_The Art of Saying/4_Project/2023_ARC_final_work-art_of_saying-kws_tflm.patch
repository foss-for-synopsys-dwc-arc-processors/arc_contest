+    m = n;
+    while (m > 512) {
+        m >>= 2;
+        cftmdl1(m, &a[n - m]);
+    }
+    cftleaf(m, 1, &a[n - m]);
+    k = 0;
+    for (j = n - m; j > 0; j -= m) {
